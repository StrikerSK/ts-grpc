{
  "name": "ts-grpc",
  "version": "1.0.0",
  "description": "Typescript application using gRPC",
  "scripts": {
    "server": "ts-node src/server/TaskServer.ts",
    "client": "ts-node src/client/TodoClient.ts",
    "node": "ts-node src/index.ts",
    "proto-gen": "yarn proto-loader-gen-types --grpcLib=@grpc/grpc-js --outDir=./src/proto/todo src/commons/proto/todo.proto"
  },
  "keywords": [
    "grpc",
    "typecript",
    "promise"
  ],
  "author": "Kristian Hanus",
  "license": "ISC",
  "devDependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.12",
    "@types/google-protobuf": "^3.15.5",
    "@types/uuid": "^8.3.1",
    "grpc-tools": "^1.11.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "grpc": "^1.24.11",
    "grpc_tools_node_protoc_ts": "^5.3.2"
  }
}
